    "lint": "tsc & eslint './src/**/*.{js,ts,tsx}' & prettier './src/**/*.{js,ts,tsx}' --check",
    "cleanup": "ts-node --project ./tsconfig.json ./src/cleanup.ts",
    "clean:temp": "rimraf temp"
