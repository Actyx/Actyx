{
  "name": "developers-actyx-com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run copy-schemas",
    "copy-schemas": "node bin/copy-schemas.js",
    "start": "npm run prebuild && docusaurus start",
    "build": "npm run lint && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "npm run lint && docusaurus deploy",
    "lint:txt:check": "textlint -f pretty-error ./docs/**/* ./blog/**/*",
    "lint:txt:fix": "textlint --fix ./docs/**/* ./blog/**/*",
    "lint:txt:dry": "textlint --dry-run ./docs/**/* ./blog/**/*",
    "lint:md:check": "npx markdownlint-cli \"./docs/**/*.md\" \"./blog/**/*\"",
    "lint:md:fix": "npx markdownlint-cli --fix \"./docs/**/*.md\" \"./blog/**/*\"",
    "lint": "npm run lint:md:check && npm run lint:txt:check"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-alpha.48",
    "@docusaurus/preset-classic": "^2.0.0-alpha.48",
    "chromium": "^2.1.2",
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-youtube": "^7.9.0",
    "textlint-rule-no-empty-section": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@textlint-rule/textlint-rule-no-unmatched-pair": "^1.0.7",
    "copy-dir": "^1.2.0",
    "markdownlint-cli": "^0.23.2",
    "mkdirp": "^1.0.4",
    "nan": "^2.14.1",
    "textlint": "^11.7.6",
    "textlint-filter-rule-comments": "^1.2.2",
    "textlint-rule-date-weekday-mismatch": "^1.0.5",
    "textlint-rule-footnote-order": "^1.0.3",
    "textlint-rule-max-comma": "^1.0.4",
    "textlint-rule-no-start-duplicated-conjunction": "^2.0.2",
    "textlint-rule-no-todo": "^2.0.1",
    "textlint-rule-period-in-list-item": "^0.3.2"
  }
}
