[package]
edition = { workspace = true }
name = "swarm-old-formats"
version = "0.1.0"
authors = ["Actyx AG"]

[dependencies]
actyx_sdk = { features = ["sqlite", "arb"], path = "../../../sdk" }
anyhow = "1.0.66"
ipfs-embed = { version = "0.26.0", default-features = false, features = ["tokio"] }
libipld = { version = "0.14", default-features = false, features = ["dag-cbor"] }
libipld12 = { package = "libipld", version = "0.12", default-features = false, features = ["unleashed", "dag-cbor"] }
quickcheck = "1.0.3"
trees = { path = "../../trees" }
