    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "cross-env": "^7.0.3",
    "eslint": "8.4.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "^27.4.5",
    "license-check-and-add": "4.0.3",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
